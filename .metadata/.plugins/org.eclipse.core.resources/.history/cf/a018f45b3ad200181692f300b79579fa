
public class Users {
	int numUsers;
	User [] nombreUsers;
	public Users () {
		nombreUsers=new User[] {new User("Javier", "javier@gmail.com", "javier123"), new User("Santiago",
				"santiperezci@gmail.com", "pass123"), new User("Daniel", "tradeos@gmail.com", "daniel123")};
		numUsers=nombreUsers.length;
	}
	public boolean verificar(User usuario) {
	
		boolean existe=false;
		for(int i=0;i<numUsers-1;i++) {
			System.out.println(nombreUsers[i].email);
	        System.out.println(nombreUsers[i].name);
	        System.out.println(nombreUsers[i].password);
			if(nombreUsers[i].name==usuario.name && nombreUsers[i].email==usuario.email && nombreUsers[i].password==usuario.password) {
				existe=true;
				//System.out.println("Dentro if");
				break;
			}
		}
		return existe;
	}
}
